<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.hp.labdbs.MainActivity"
    tools:showIn="@layout/activity_main"
    android:background="@drawable/a">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name"
        android:id="@+id/textView2"
        android:layout_below="@+id/editText"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#e31111"
        android:textSize="@dimen/abc_dialog_padding_material" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText"
        android:layout_marginTop="49dp"
        android:layout_below="@+id/textView9"
        android:background="#f02323"
        android:contextClickable="false"
        android:textColor="#0d0101"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText2"
        android:layout_below="@+id/editText"
        android:background="#94a8ea"
        android:textColor="#0e0202"
        android:layout_alignBottom="@+id/textView2"
        android:layout_toRightOf="@+id/button6"
        android:layout_toEndOf="@+id/button6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Phoneno"
        android:id="@+id/textView3"
        android:layout_below="@+id/editText2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#e21515"
        android:textSize="@dimen/abc_dialog_padding_material"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Street"
        android:id="@+id/textView4"
        android:textColor="#e62424"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_alignTop="@+id/spinner"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="city"
        android:id="@+id/textView5"
        android:layout_below="@+id/spinner"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#ea2121"
        android:textSize="@dimen/abc_dialog_padding_material"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Service"
        android:id="@+id/textView6"
        android:textColor="#dc2b2b"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/spinner2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="State"
        android:id="@+id/textView7"
        android:textColor="#e31717"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:layout_below="@+id/spinner3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_below="@+id/editText2"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2"
        android:background="#e31717"
        android:textColor="#0f0202"/>

    <Spinner
        android:layout_width="fill_parent"
        android:layout_height="35dp"
        android:id="@+id/spinner"
        android:layout_below="@+id/editText3"
        android:entries="@array/street"
        android:layout_alignLeft="@+id/spinner3"
        android:layout_alignStart="@+id/spinner3"
        android:background="#94a8ea"
        android:popupBackground="#e60c0c" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:id="@+id/spinner2"
        android:layout_below="@+id/spinner"
        android:layout_alignRight="@+id/spinner"
        android:layout_alignEnd="@+id/spinner"
        android:entries="@array/city"
        android:layout_alignLeft="@+id/spinner3"
        android:layout_alignStart="@+id/spinner3"
        android:background="#f02323"
        android:contentDescription="@string/abc_font_family_display_1_material"
        android:contextClickable="false"
        android:popupBackground="#94a8ea" />

    <Spinner
        android:layout_width="fill_parent"
        android:layout_height="35dp"
        android:id="@+id/spinner3"
        android:layout_below="@+id/spinner2"
        android:entries="@array/service"

        android:background="#94a8ea"

        android:visibility="visible"
        android:popupBackground="#ee1616"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignLeft="@+id/editText3"
        android:layout_alignStart="@+id/editText3" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="@dimen/abc_action_button_min_width_overflow_material"
        android:id="@+id/spinner4"
        android:entries="@array/state"
        android:layout_above="@+id/textView8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#f20f0f"
        android:dropDownSelector="#b22c2c"
        android:popupBackground="#94a8ea"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignLeft="@+id/spinner5"
        android:layout_alignStart="@+id/spinner5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="country"
        android:id="@+id/textView8"
        android:textColor="#e60e0e"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textIsSelectable="false"
        android:textSize="@dimen/abc_dialog_padding_material" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:id="@+id/spinner5"
        android:entries="@array/country"
        android:layout_alignTop="@+id/textView8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:background="#94a8ea"
        android:popupBackground="#e81d1d"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignLeft="@+id/spinner3"
        android:layout_alignStart="@+id/spinner3" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/button"
        android:background="@android:drawable/ic_menu_add"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7"
        android:layout_alignTop="@+id/button3" />

    <Button
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/button2"
        android:background="@android:drawable/ic_input_delete"
        android:layout_alignTop="@+id/button3"
        android:layout_toLeftOf="@+id/button5"
        android:layout_toStartOf="@+id/button5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:background="@android:drawable/ic_menu_delete"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button"
        android:layout_toRightOf="@+id/button" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:background="@android:drawable/ic_menu_search"
        android:layout_alignTop="@+id/button2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="ID"
        android:id="@+id/textView"
        android:layout_alignTop="@+id/editText"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:textColor="#ed1616"
        android:textSize="@dimen/abc_dialog_padding_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#eb1818"
        android:textSize="@dimen/abc_action_button_min_width_material"
        android:text="ENTER DATA" />

    <Button
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/button6"
        android:background="@android:drawable/arrow_down_float"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/button3"
        android:layout_toEndOf="@+id/button3" />

</RelativeLayout>
